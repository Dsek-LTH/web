{
  "name": "dsek-web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install && svelte-kit sync && zenstack generate && prisma generate && pnpm paraglide && pnpm snaplet-seed generate",
    "setup-dev": "vite-node dev/setup.ts",
    "ci-check": "concurrently 'pnpm:lint' 'pnpm:format' 'pnpm:test' 'pnpm:check' --prefix-colors 'auto' --group",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "pm2 start prod/ecosystem.config.cjs --no-daemon",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "pnpm test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest run --retry 2",
    "generate": "zenstack generate && prisma generate",
    "migrate": "zenstack generate && prisma migrate dev && snaplet-seed sync",
    "seed": "prisma db seed",
    "prisma-case-format": "prisma-case-format -f src/database/prisma/schema.prisma",
    "paraglide": "paraglide-js compile --project ./project.inlang --outdir ./src/translations/paraglide",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "zenstack": {
    "schema": "src/database/schema.zmodel"
  },
  "prisma": {
    "schema": "src/database/prisma/schema.prisma",
    "seed": "vite-node src/database/seed/main.ts"
  },
  "@snaplet/seed": {
    "config": "src/database/seed/seed.config.ts"
  },
  "devDependencies": {
    "@clack/prompts": "^0.7.0",
    "@event-calendar/core": "^3.4.0",
    "@event-calendar/resource-timeline": "^3.4.0",
    "@faker-js/faker": "^8.4.1",
    "@iconify-json/flag": "^1.1.53",
    "@iconify-json/mdi": "^1.1.67",
    "@inlang/paraglide-js": "1.1.0",
    "@inlang/paraglide-js-adapter-sveltekit": "^0.4.1",
    "@snaplet/copycat": "^5.0.0",
    "@snaplet/seed": "0.98.0",
    "@sveltejs/adapter-node": "^5.2.0",
    "@sveltejs/adapter-vercel": "^5.4.1",
    "@sveltejs/kit": "^2.5.20",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tailwindcss/typography": "^0.5.13",
    "@types/eslint": "^8.56.11",
    "@types/node": "^22.1.0",
    "@types/node-schedule": "^2.1.7",
    "@types/nodemailer": "^6.4.15",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@whiterussianstudio/tailwind-easing": "^1.0.0",
    "@zenstackhq/openapi": "^1.12.5",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "daisyui": "^4.12.10",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-svelte": "^2.43.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.40",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.21.1",
    "prisma-case-format": "^2.2.1",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.5",
    "sveltekit-flash-message": "^2.4.4",
    "sveltekit-superforms": "^2.16.1",
    "tailwindcss": "^3.4.7",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-node": "^2.0.5",
    "vitepress": "^1.3.4",
    "vitest": "^2.0.5",
    "vitest-mock-extended": "^2.0.0",
    "zenstack": "^1.12.5",
    "zod": "^3.23.8"
  },
  "type": "module",
  "dependencies": {
    "@auth/core": "^0.34.2",
    "@auth/sveltekit": "^1.4.2",
    "@directus/sdk": "^19.0.1",
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@fontsource-variable/inter": "^5.0.20",
    "@fontsource/ibm-plex-mono": "^5.0.13",
    "@fontsource/lexend": "^5.0.20",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@keycloak/keycloak-admin-client": "^23.0.7",
    "@playwright/test": "^1.48.1",
    "@prisma/client": "^5.21.1",
    "@shlinkio/shlink-js-sdk": "^1.2.0",
    "@stripe/stripe-js": "^3.5.0",
    "@tiptap/core": "^2.5.8",
    "@tiptap/pm": "^2.5.8",
    "@tiptap/starter-kit": "^2.5.8",
    "@vercel/speed-insights": "^1.0.12",
    "@zenstackhq/runtime": "^1.12.5",
    "@zenstackhq/server": "^1.12.5",
    "croppie": "^2.6.5",
    "dayjs": "^1.11.12",
    "embla-carousel-autoplay": "^8.1.8",
    "embla-carousel-svelte": "^8.1.8",
    "expo-server-sdk": "^3.10.0",
    "express": "^4.19.2",
    "ics": "^3.7.6",
    "isomorphic-dompurify": "^2.16.0",
    "markdown-to-txt": "^2.0.1",
    "marked": "^11.2.0",
    "meilisearch": "^0.45.0",
    "minio": "^7.1.3",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.14",
    "sharp": "^0.33.4",
    "stripe": "^14.25.0",
    "svelte-easy-crop": "^2.0.4",
    "svelte-stripe": "^1.2.0",
    "tailwind-merge": "^2.4.0",
    "tiptap-markdown": "^0.8.10",
    "uuid": "^10.0.0",
    "vite-plugin-cjs-interop": "^2.1.1"
  },
  "pnpm": {
    "patchedDependencies": {
      "devalue@4.3.2": "patches/devalue@4.3.2.patch",
      "devalue@5.0.0": "patches/devalue@5.0.0.patch"
    }
  },
  "engines": {
    "node": "<=23"
  },
  "packageManager": "pnpm@9.12.2"
}
